{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "settings-window"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:allow-open",
    "core:webview:allow-create-webview-window",
    "core:window:allow-close",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "core:event:allow-emit",
    "store:default"
  ]
}
